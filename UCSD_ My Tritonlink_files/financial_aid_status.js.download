function showFAStatus() {
	var url = "financialaidstatus.htm";
	var profilewindow = window
			.open(
					url,
					'_blank',
					'width=600,height=300,toolbar=no,menubar=no, location=no, scrollbars=no, resizable=yes, directories=no');
	profilewindow.focus();
}
function openSelectionPage(url) {
	if (navigator.userAgent.indexOf("Chrome") != -1 || navigator.userAgent.indexOf("Firefox") != -1){
		window.open(url);
	} else  {
		window.open(url,'_blank','width=1850,height=1800,toolbar=no,menubar=no, location=no, scrollbars=yes, resizable=yes, directories=no');
	}
}